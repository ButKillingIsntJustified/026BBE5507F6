ExtraVariables.ToolButton = Main.InnerMain.PropCreation.ToolSelect:WaitForChild('HatName'):Clone()
Main.InnerMain.PropCreation.ToolSelect.HatName:Destroy()
ExtraVariables.AligningHat = {nil}
ExtraVariables.Presets = {}
for i,v in pairs(ExtraVariables.HatsToDisplay)do
	local Handle = v.Handle:Clone()
	local newVPF = ExtraVariables.ToolButton:Clone()
	newVPF.Text = ""
	Handle.Parent = newVPF.ToolDisplay
	Handle.Anchored = true
	local Camera = Create("Camera",newVPF.ToolDisplay,{CFrame = CFrame.new(Handle.Position)*CFrame.new(0,0,5)})
	local Surround = 0
	game:GetService("RunService").RenderStepped:Connect(function()
		Surround=Surround+1
		Camera.CFrame = CFrame.new(Handle.Position)*CFrame.Angles(0,math.rad(Surround),0)*CFrame.new(0,1.25,2)
		Camera.CFrame = Camera.CFrame * CFrame.Angles(-math.rad(40/1.25),0,0)
		Camera.FieldOfView = 60
		if Surround>9e9 then
			Surround=1
		end
	end)
	newVPF.ToolDisplay.CurrentCamera=Camera
	newVPF.Parent = Main.InnerMain.PropCreation.ToolSelect
	ExtraVariables.AligningHat={v.Handle,v}
	OnClick(newVPF,function()
		ExtraVariables.AligningHat = {Handle,v}
		Main.InnerMain.PropCreation.Visible = true
		Main.InnerMain.PropCreation.Initialize.Visible = true
		Main.InnerMain.PropCreation.ToolSelect.Visible = false
		for i,v in pairs(Main.InnerMain.PropCreation.Initialize.Tool.ToolVPF:GetChildren())do
			if v:IsA("BasePart") then
				v:Destroy()
			end
		end
		Main.InnerMain.PropCreation.Initialize.Tool.ToolVPF.CurrentCamera=Camera
		Handle:Clone().Parent = Main.InnerMain.PropCreation.Initialize.Tool.ToolVPF
	end)
end
ExtraVariables.EquipFromOffset = nil
ExtraVariables.Initialize=Main.InnerMain.PropCreation.Initialize.Tool
OnClick(ExtraVariables.Initialize.Yes,function()
	local Display1 = ExtraVariables.AligningHat[1]:Clone()
	pcall(function()
		Display1.AccessoryWeld:Destroy()
	end)
	Display1.Parent = workspace.CurrentCamera
	Display1.Transparency = 0.3
	Display1.Anchored = true
	Display1.CanCollide = false
	local Triggered = false
	local repositioning2 = false
	local maneuver = CFrame.new(0,0,0)
	local Selfex=nil
	local LetGo = nil
	Selfex=Services.UIS.InputBegan:Connect(function(key,gp)
		if Services.UIS:GetFocusedTextBox()==nil then
			if key.KeyCode==Enum.KeyCode.ButtonR2 then
				Triggered = true
				local ObjectSpace = Display1.CFrame:ToObjectSpace(Parts.RightArm.CFrame):Inverse()
				local theTool = ExtraVariables.CreateHatAsTool("T:"..ExtraVariables.AligningHat[2].Name,ExtraVariables.AligningHat[2].Name,ObjectSpace,true)
				theTool.Parent = Character
				ExtraVariables.Presets[ExtraVariables.AligningHat[2]] = theTool
				Destroy(Display1,-1)
				Selfex:Disconnect()
			elseif key.KeyCode==Enum.KeyCode.ButtonR1 then
				repositioning2 = true
				maneuver = Display1.CFrame:ToObjectSpace((Camera.CFrame*Services.VR:GetUserCFrame(Enum.UserCFrame.RightHand))):Inverse()
			end
		end
	end)
	LetGo=Services.UIS.InputEnded:Connect(function(key,gp)
		if Services.UIS:GetFocusedTextBox()==nil then
			if key.KeyCode==Enum.KeyCode.ButtonR1 then
				repositioning2 = false
				maneuver = Display1.CFrame:ToObjectSpace((Camera.CFrame*Services.VR:GetUserCFrame(Enum.UserCFrame.Head)*CFrame.new(0.5,-0.5,-2))):Inverse()
			end
		end
	end)
	local Positioner = nil
	local ReposInterv = 0
	Display1.CFrame = (Camera.CFrame*Services.VR:GetUserCFrame(Enum.UserCFrame.Head)*CFrame.new(0.5,-0.5,-2))
	Positioner = game:GetService('RunService').RenderStepped:Connect(function()
		ExtraVariables.Paused = true
		if Triggered then
			ExtraVariables.ActionOpened = true
			HoldingDirection = false
			PredictingPosition = false
			ExtraVariables.Paused = false
			LetGo:Disconnect()
			Positioner:Disconnect()
		else
			if Display1 then
				ExtraVariables.ActionOpened = false
				HoldingDirection = false
				PredictingPosition = false
				if maneuver==CFrame.new(0,0,0) then
					Display1.CFrame = (Camera.CFrame*Services.VR:GetUserCFrame(Enum.UserCFrame.Head))*CFrame.new(0.5,-0.5,-2)
				else
					if repositioning2 then
						Display1.CFrame = (Camera.CFrame*Services.VR:GetUserCFrame(Enum.UserCFrame.RightHand)*maneuver)
					else
						Display1.CFrame = (Camera.CFrame*Services.VR:GetUserCFrame(Enum.UserCFrame.Head)*CFrame.new(0.5,-0.5,-2))*maneuver
					end
				end
			end
		end
	end)
end)
