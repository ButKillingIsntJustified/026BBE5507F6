	local Hour = os.date("%I")
	if Hour:sub(1,1)=="0" then
		Hour = Hour:sub(2,#Hour)
	end
	ExtraVariables.Clock.Text = Hour .. ":" .. os.date("%M") .. ":" .. os.date("%S") .. " " .. os.date("%p")
	VrSurface.Parent = User:FindFirstChildWhichIsA('PlayerGui')
	VrSurface.LightInfluence = 0
	VrSurface.Adornee = MenuPart
