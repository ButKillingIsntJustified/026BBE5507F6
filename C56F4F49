script.Disabled = true
local VR = game:GetService('VRService')

if _G.IsVr == true then return end
_G.IsVr = true

repeat _G.IsVr = true task.wait(-1) until game:GetService('Players').LocalPlayer and game:GetService('Players').LocalPlayer.Character and game:GetService('Players').LocalPlayer.Character:FindFirstChild('Head')

_G.BrewVersion = "v1.332"
local Advanced = {
	Scripts_To_Remove = {
		"NexusVRCharacterModelClientLoader",
		"VR",
	};
	RemoveLocals = true;
	GamesList = {
		{6708206173,"Rate My Avatar"},
		{7115420363,"The Booth Plaza"},
		{155615604,"Prison Life"},
		{189707,"Natural Disaster"},
