	MenuPart.CFrame = MenuPart.CFrame:Lerp(PlaceCFrame,0.06)
	if ExtraVariables.ActionOpened == true then
		MenuPart.Parent = Camera
		if MenuPart.Size ~= ExtraVariables.MenuOpen then
			Services.TweenService:Create(MenuPart,TweenInfo.new(0.15),{Size = ExtraVariables.MenuOpen}):Play()
		end
	else
		if MenuPart.Size.Magnitude > 0.01 then
			Services.TweenService:Create(MenuPart,TweenInfo.new(0.15),{Size = Vector3.new(0,0,0)}):Play()
		end
		MenuPart.Parent = nil
	end
end)
