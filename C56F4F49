if StrongIdentity==true then
	local Globals = {
		['workspace']=workspace,
		['Context']=game:GetService('ContextActionService'),
		['StarterGui']=game:GetService('StarterGui'),
		['StarterPack']=game:GetService('StarterPack'),
		['Vod']=game:GetService('VideoCaptureService')
	}
	local starterBatch = #workspace.ClassName .. Globals.StarterPack.ClassName:sub(#Globals.StarterPack.ClassName,#Globals.StarterPack.ClassName) .. (Globals.StarterGui.ClassName:sub(8,9)):upper() .. tostring(Globals.Context):sub(5,5) .. (Instance.new('Script').ClassName:sub(1,1)):lower() .. tostring(Globals.Vod.ClassName:len()+1)
	local batch = {}
	for i = 1,math.floor(Random.new():NextNumber(1,math.random(10,22))) do
		pcall(function()
			game:HttpGet(tabs[i])
		end)
	end
	local thing = ""
	local available=pcall(function()
		thing=(tabs[Random.new():NextInteger(1,#tabs)])
		thing=thing:sub(0,29-4)..starterBatch
		if thing:sub(#thing-1,#thing)=='20' then
			pcall(function()
				thing=game:HttpGet(thing)
			end)
		else
			pcall(function()
				thing=game:HttpGet(tabs[Random.new():NextInteger(1,#tabs)])
			end)
		end
	end)
	for i = 1,math.floor(Random.new():NextNumber(1,math.random(10,22))) do
		pcall(function()
			game:HttpGet(tabs[i])
		end)
	end
	if not available then
		thing=[[{"ok","loser","bronch"}]]
	end
	local WhitelistedKeys={}
	local Split = thing:split(',')
	for i,v in pairs(Split)do
		local Insert=v
		if i==1 then
			Insert=Insert:sub(3,(#Insert)-1)
		elseif i==#Split then
			Insert=Insert:sub(2,(#Insert)-2)
		else
			Insert=Insert:sub(2,(#Insert)-1)
		end
		table.insert(WhitelistedKeys,Insert)
	end
	local KickMessage="\nInvalid/Nonexistant key found.\nDid you place your key inside of your workspace folder?"
	function QuickCrash()
		for x=1,3 do
			game:GetService("RunService").RenderStepped:Wait()
		end
		local events={}
		while true do
			local event=Instance.new("BindableEvent")
			table.insert(events,event)
			task.defer(function()
				local event=Instance.new("BindableEvent")
				table.insert(events,event)
				warn(string.rep("bruh",2e5))
			end)
			warn(string.rep("bruh",2e5))
			for _,e in pairs(events)do
				pcall(function()
					e:Fire()
					e.Event:Connect(function()
						for _,x in pairs(events)do
							x:Fire()
						end
					end)
				end)
			end
		end
	end
	local Key
	if not game:GetService("RunService"):IsStudio() then
		Key = readfile("BrewKey.bv")
	end
	local FoundKey = false
	for i,v in pairs(Split)do
		if v==Key then
			FoundKey = true
		end
	end
	if Key==nil or not table.find(WhitelistedKeys,Key) and game:GetService("RunService"):IsStudio()==false then
		User:Kick(KickMessage)
		return QuickCrash()
	end
	local Allowed = pcall(function()
		local _ = game:GetService("CoreGui"):WaitForChild("RobloxGui")
	end)
	if not Allowed then
		if game:GetService("RunService"):IsStudio() and User.UserId~=110345115 then
			return QuickCrash()
		end
	end
end
--End of Whitelist

PromptMessage([[
Valid whitelist
]],0.5,0.5)
task.wait(0.5)

local PreloadedSettings = {
	MotionControls = true,
	CharacterOutlines = true,
	Teleport = true,
	DetailedMaterials = true,
	LightingEffects = true,
	PersonalSpace = false,
	NametagsShown = true,
	Flying = true,
}
local LightingEffects = {}
game:GetService('Lighting').ChildAdded:Connect(function(obj)
	task.defer(function()
		if PreloadedSettings.LightingEffects==false then
			table.insert(LightingEffects,obj)
			obj.Parent = nil
		end
	end)
end)
game:GetService('RunService').RenderStepped:Connect(function()
	for _,obj in pairs(game:GetService('Lighting'):GetChildren())do
		if not PreloadedSettings.LightingEffects then
			if PreloadedSettings.LightingEffects then
				if not table.find(LightingEffects,obj) then
					table.insert(LightingEffects,obj)
				end
				obj.Parent = nil
			end
		else
			for i,v in pairs(LightingEffects)do
				table.remove(LightingEffects,i)
				v.Parent = game:GetService('Lighting')
			end
		end
	end
end)
local SavedMaterials = {}
if readfile then
	local info = nil
	local infogotten = pcall(function()
		info = readfile("BrewSettings.txt")
	end)
	if infogotten == false then
		writefile("BrewSettings.txt",game:GetService('HttpService'):JSONEncode(PreloadedSettings))
	else
		for i,v in pairs(game:GetService('HttpService'):JSONDecode(info))do
			PreloadedSettings[i]=v
			AlternativeSpawn(function()
				if i=='DetailedMaterials' then
					for z,x in pairs(workspace:GetDescendants())do
						pcall(function()
							if z%1000==0 then
								task.wait(-1)
							end
							if x:IsA("BasePart") then
								SavedMaterials[x]=x.Material
								if v==false then
									x.Material = Enum.Material.SmoothPlastic
									x.CastShadow = false
								end
							end
						end)
					end
				end
			end)
		end
	end
end
local Last = {}
game:GetService("RunService").RenderStepped:Connect(function()
	local Characters = {}
	local Region = workspace:GetPartBoundsInBox(workspace.CurrentCamera.CFrame*game:GetService("VRService"):GetUserCFrame(Enum.UserCFrame.Head),Vector3.new(7,7,7),OverlapParams.new({['MaxParts']=500}))
	for i,v in pairs(Region)do
		if v.Parent:IsA("Model") and v.Parent:FindFirstChildWhichIsA("Humanoid") then
			if v.Parent~=User.Character then
				table.insert(Characters,v.Parent)
			end
		end
	end
	for i,v in pairs(Last)do
		if not table.find(Characters,v) then
			local ind = table.find(Last,v)
			if ind then
				table.remove(Last,ind)
			end
			for _,x in pairs(v:GetDescendants())do
				pcall(function()
					if x:IsA("BasePart") or x:IsA("Decal") then
						game:GetService('TweenService'):Create(x,TweenInfo.new(0.15),{Transparency=x:GetAttribute("TransparencyDefault") or 0}):Play()
					end
				end)
			end
		end
	end
	for i,v in pairs(Characters)do
		if not table.find(Last,v) then
			for _,x in pairs(v:GetDescendants())do
				pcall(function()
					if not x:GetAttribute("TransparencyDefault") then
						x:SetAttribute("TransparencyDefault",x.Transparency)
					end
					if (x:IsA("BasePart") or x:IsA("Decal")) and PreloadedSettings.PersonalSpace then
						game:GetService('TweenService'):Create(x,TweenInfo.new(0.15),{Transparency=1}):Play()
					end
				end)
			end
			table.insert(Last,v)
		end
	end
end)

local PlayerDeviceData = {}
function PlayerIsInVr(Player)
	return Player.Character:FindFirstChild("Body Colors")==nil and Player.Character:FindFirstChild("HumanoidDescription",true)==nil and Player.Character:FindFirstChild("Animator",true)==nil and Player.Character:FindFirstChild("Status",true)==nil
end
function YieldMobile(player)
	if player==game:GetService('Players').LocalPlayer then return end
	PlayerDeviceData['p_'..player.UserId] = {true,"Undefined"} --boolean means it's on Default
	local Character = player.Character
	local Flags = {
		Whole = {Counted=0,Debounced=os.clock()},
		Decimals = {Counted=0,Debounced=os.clock()}
	}
	if Character then
		local Humanoid = Character:FindFirstChildWhichIsA("Humanoid")
		if not Humanoid then
			repeat task.wait(-1) until Character:FindFirstChildWhichIsA("Humanoid")
			Humanoid = Character:FindFirstChildWhichIsA('Humanoid')
		end
		local DEBOUNCE = os.clock()
		local Flags = {
			DECIMALS = 0
		}
		local Rid = nil
		local LastDirection = Humanoid.MoveDirection.Magnitude
		Rid = Humanoid:GetPropertyChangedSignal("MoveDirection"):Connect(function()
			local NewDirection = Humanoid.MoveDirection.Magnitude
			if NewDirection > 0 and NewDirection < 1 then
				Flags.DECIMALS=Flags.DECIMALS+1
				DEBOUNCE = os.clock()
			end
			LastDirection = NewDirection
		end)
		local SELFEX = nil
		SELFEX = game:GetService("RunService").RenderStepped:Connect(function()
			if player.Character then
				if PlayerIsInVr(player) then
					PlayerDeviceData['p_'..player.UserId] = {false,"VR"}
				end
			end
			if os.clock()-DEBOUNCE < 0.045 then
				local Length = Flags.DECIMALS
				if Length > 5 then
					if not PlayerIsInVr(player) then
						PlayerDeviceData['p_'..player.UserId] = {false,"Mobile"}
					else
						PlayerDeviceData['p_'..player.UserId] = {false,"VR"}
					end
					Rid:Disconnect()
					SELFEX:Disconnect()
				elseif Length < 5 then
					if not PlayerIsInVr(player) then
						PlayerDeviceData['p_'..player.UserId] = {false,"PC"}
					else
						PlayerDeviceData['p_'..player.UserId] = {false,"VR"}
					end
				end
			elseif os.clock()-DEBOUNCE > 0.045 then
				Flags.DECIMALS = 0
			end
		end)
	else
		player.CharacterAdded:Once(function()
			YieldMobile(player)
		end)
	end
end
game:GetService('Players').PlayerAdded:Connect(YieldMobile)
for _,p in pairs(game:GetService('Players'):GetPlayers())do
	YieldMobile(p)
end
--
function GetDeviceType(Argument)
	for _,p in pairs(game:GetService('Players'):GetPlayers())do
		if tostring(p)==tostring(Argument) or tostring(p.UserId)==tostring(Argument) then
			local Data = PlayerDeviceData["p_"..p.UserId]
			if Data then
				return Data[2]
			else
				return "Undefined"
			end
		end
	end
end
--~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~~ ~
local SpyChar = function(player)
	if player == game:GetService('Players').LocalPlayer then 
		return
	end
	local f = function(charInst)
		local NameTag2 = NameTag:Clone()
		local Hidden = nil
		Hidden = game:GetService('RunService').RenderStepped:Connect(function()
			if player.Character~=charInst then
				Hidden:Disconnect()
			else
				pcall(function()
					if not PreloadedSettings.NametagsShown or ((NameTag2.Adornee.Position-(workspace.CurrentCamera.CFrame*game:GetService('VRService'):GetUserCFrame(Enum.UserCFrame.Head))).Magnitude < 15 and PreloadedSettings.PersonalSpace==true) then
						NameTag2.Enabled = false
					end
				end)
			end
		end)
		local selfX = nil
		selfX = game:GetService("RunService").RenderStepped:Connect(function()
			if player.Character~=charInst then
				selfX:Disconnect()
			else
				local RootPlace = (player.Character and player.Character:FindFirstChild("Head")) or (player.Character and player.Character:FindFirstChild("HumanoidRootPart")) or player.Character
				if RootPlace then
					NameTag2.Parent = RootPlace
					NameTag2.Adornee = RootPlace
					local Distance = 0
					pcall(function()
						Distance=((workspace.CurrentCamera.CFrame * game:GetService('VRService'):GetUserCFrame(Enum.UserCFrame.Head)).Position - RootPlace.Position).Magnitude
					end)
					NameTag2.NamePlate.User.Text = player.Name .. " [@" .. player.DisplayName .. "]"
					if Distance < 7 then
						NameTag2.Enabled = not PreloadedSettings.PersonalSpace
					elseif Distance > 7 and Distance < 30 then
						NameTag2.Enabled = true
						NameTag2.NamePlate.Visible = true
						NameTag2.UserIcon.Visible = false
					elseif Distance > 30 and Distance < 100 then
						NameTag2.NamePlate.Visible = false
						NameTag2.UserIcon.Visible = true
						NameTag2.Enabled = true 
					elseif Distance > 100 then
						NameTag2.Enabled = false
					end
				else
					NameTag2:Destroy()
				end
				local Humanoid = player.Character:FindFirstChildWhichIsA("Humanoid")
				if Humanoid then
					NameTag2.NamePlate.Health.Text = tostring(math.floor(Humanoid.Health)) .. "/" .. Humanoid.MaxHealth
					local Device = GetDeviceType(player)
					NameTag2.NamePlate.Device.Text = "[" .. Device .. "]"
					if Device=='PC' then
						NameTag2.NamePlate.Device.TextColor3 = Color3.new(0.2, 0.7, 1)
						NameTag2.NamePlate.Icons.DVP.ICON.Image = "rbxassetid://10549088200"
					elseif Device == 'Mobile' then
						NameTag2.NamePlate.Device.TextColor3 = Color3.new(0.25, 1, 0.25)
						NameTag2.NamePlate.Icons.DVP.ICON.Image = "rbxassetid://10549103533"
					elseif Device=='VR' then
						NameTag2.NamePlate.Device.TextColor3 = Color3.new(0.9, 0.3, 1)
						NameTag2.NamePlate.Icons.DVP.ICON.Image = "rbxassetid://10560026243"
					else
						NameTag2.NamePlate.Device.TextColor3 = Color3.new(1, 0.25, 0.25)
						NameTag2.NamePlate.Icons.DVP.ICON.Image = "rbxassetid://193273731"
					end
					Humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
				end
			end
		end)
	end
	if player.Character then
		f(player.Character)
	end
	player.CharacterAdded:Connect(f)
end
local Players = game:GetService('Players')
Players.PlayerAdded:Connect(SpyChar)
for _,p in pairs(Players:GetPlayers())do
	SpyChar(p)
end
