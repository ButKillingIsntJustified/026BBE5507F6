SetSettingCheckBox(SettingsMenu.Choice5,"Teleport")
SetSettingCheckBox(SettingsMenu.Choice6,"NametagsShown")
SetSettingCheckBox(SettingsMenu.Choice7,"DetailedMaterials",function(Boolean)
	for Part,Material in pairs(SavedMaterials)do
		if Boolean == true then
			Part.Material=Material
		else
			Part.Material=Enum.Material.SmoothPlastic
		end
	end
end)
