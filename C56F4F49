--//Tools
local HatsAsTools={}

function CreateHatAsTool(ToolName,Hat,Offset,PlaceBackWhenUnequipped) 
	local NewTool = Create('Tool',nil,{Name=ToolName,RequiresHandle=true})
	for i,v in pairs(Character:GetChildren())do
		if v.Name==Hat or v==Hat and v:IsA("Accessory") then
			Hat=v
			break
		end
	end
	if type(Hat)=='string' then
		return nil
	end
	if ExtraVariables.LocalProps[Hat]~=nil then
		ExtraVariables.LocalProps[Hat].GripOffset.Value = Offset
		return ExtraVariables.LocalProps[Hat]
	end
	ExtraVariables.LocalProps[Hat] = NewTool
	local Handle = Hat:FindFirstChild("Handle")
	Netless(Handle)
	if Handle:FindFirstChild("AccessoryWeld") then
		Create("CFrameValue",Handle,{Value=Handle.AccessoryWeld.C0,Name="C0"})
		Create("CFrameValue",Handle,{Value=Handle.AccessoryWeld.C1,Name="C1"})
		Create("CFrameValue",Handle,{Value=Handle.AccessoryWeld.Part0.CFrame:ToObjectSpace(Handle.AccessoryWeld.Part1.CFrame):Inverse(),Name="WeldOffset"})
		Create("ObjectValue",Handle,{Value=Handle.AccessoryWeld.Part0,Name="Part0"})
		Create("ObjectValue",Handle,{Value=Handle.AccessoryWeld.Part1,Name="Part1"})
		Handle.AccessoryWeld:Destroy()
	end
	if NewTool:FindFirstChild("GripOffset")==nil then
		NewTool:GetPropertyChangedSignal("Parent"):Connect(function()
			if NewTool.Parent==Character then
				Handle.Transparency=0
			else
				Handle.Transparency=1
			end
		end)
	end
	local Grip = NewTool:FindFirstChild("GripOffset") or Create("CFrameValue",NewTool,{Value = Offset,Name="GripOffset"})
	game:GetService("RunService").RenderStepped:Connect(function()
		if NewTool.Parent==Character then
			Handle.Transparency=0
			Handle.CFrame = Parts.RightArm.CFrame*Grip.Value
			task.defer(function()
				Handle.CFrame = Parts.RightArm.CFrame*Grip.Value
			end)
		else
			Handle.Transparency=1
			if PlaceBackWhenUnequipped then
				Handle.CFrame = Handle.Part1.Value.CFrame * Handle.WeldOffset.Value
				task.defer(function()
					Handle.CFrame = Handle.Part1.Value.CFrame * Handle.WeldOffset.Value
				end)
			else
				Handle.CFrame = Character.Head.CFrame*CFrame.new(0,-2,0)
			end
		end
	end)
	return NewTool
end
ExtraVariables.CreateHatAsTool = CreateHatAsTool
CreateHatAsTool("Give an L","Big L",CFrame.new(-0.0206413269, -1.05122375, 0.875359535, 1, 0, 0, 0, -4.37113847e-08, -1, 0, 0.999999881, -4.37113883e-08),false)
CreateHatAsTool("Pistol","1911 Right",CFrame.new(-0.00897979736, 1.30259275, -0.496437073, -1, 0, 8.74227766e-08, 0, 0.999999881, 0, -8.74227766e-08, 0, -1),true)
CreateHatAsTool("Knife","Meshes/KnifeFaceAccessory",CFrame.new(-0.88650322, 1.13049304, 0.0296177864, 0, 0, -1, 0, 1, 0, 1, 0, 0),false,CFrame.Angles(0,math.rad(90),0))
CreateHatAsTool("Big Hammer","MARTILLO",CFrame.new(-0.976185083, -0.770004272, 0.040807724, -9.27258839e-08, -1, 1.1920929e-07, -9.27258839e-08, -2.08616228e-07, -1.00000012, 1.00000036, -9.27258839e-08, -9.27258696e-08),true)
