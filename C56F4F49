function SetSettingCheckBox(obj,boolean,func)
	obj=obj.CheckBox
	if PreloadedSettings[boolean] == true then
		obj.Text = Check
	elseif PreloadedSettings[boolean] == false then
		obj.Text = ""
	end
	OnClick(obj,function()
		if obj.Text==Check then
			obj.Text = ""
			PreloadedSettings[boolean]=false
		else
			obj.Text = Check
			PreloadedSettings[boolean]=true
		end
		writefile("BrewSettings.txt",game:GetService('HttpService'):JSONEncode(PreloadedSettings))
		if func then
			func(PreloadedSettings[boolean])
		end
	end)
	pcall(function()
		func(PreloadedSettings[boolean])
	end)
end
