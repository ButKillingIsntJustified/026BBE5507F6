local LayedOut=0
local Label = Main.InnerMain.Chat.ChatLogs.FreshLog
Label.Parent=nil
Label=Label:Clone()
ExtraVariables.CreateLabelInChat=function(Text,Color)
	LayedOut=LayedOut+1
	local Info = Label:Clone()
	Info.Text = Text
	Info.TextColor3=Color
	Info.Parent = Main.InnerMain.Chat.ChatLogs
	Info.LayoutOrder = LayedOut
	task.delay(60*5,function()
		Info:Destroy()
	end)
end
game:GetService('RunService').RenderStepped:Connect(function()
	local Abc = 0
	for i,v in pairs(Main.InnerMain.Chat.ChatLogs:GetChildren())do
		if v:IsA("TextLabel") then
			Abc=Abc+v.AbsoluteSize.Y
		end
	end
	Main.InnerMain.Chat.ChatLogs.CanvasPosition = Vector2.new(0,Abc)
	Main.InnerMain.Chat.ChatLogs.CanvasSize = UDim2.new(0,0,0,Abc)
end)
function DidChat(player)
	if player:IsFriendsWith(User.UserId) then
		ExtraVariables.CreateLabelInChat("Your friend, '"..player.Name .. " (@" ..player.DisplayName ..")' joined your game.",Color3.new(0,1,0))
	elseif player==User then
		ExtraVariables.CreateLabelInChat("Brew VR was created by Amokah (aka VulpineQueen) - Please do not share.",Color3.new(1,1,0))
	end
	Main.InnerMain.Chat.ChatLogs.CanvasSize = UDim2.new(0,0,0,GetLabels())
	Main.InnerMain.Chat.ChatLogs.CanvasPosition = Vector2.new(0,GetLabels())
	local selfex = nil
	selfex = game:GetService('Players').PlayerRemoving:Connect(function(ins)
		if ins==player then
			selfex:Disconnect()
			ExtraVariables.CreateLabelInChat("Player, '"..player.Name .. " (@" .. player.DisplayName..")' has left the game.",Color3.new(1,0.35,0.35))
		end
	end)
	player.Chatted:Connect(function(msg)
		ExtraVariables.CreateLabelInChat("[" .. player.Name .. "]: "..msg,Color3.new(1,1,1))
	end)
end
local CB = Main.InnerMain.Chat.ChattingBar
function GetLabels()
	local Scale = 0
	for i,v in pairs(Main.InnerMain.Chat.ChatLogs:GetChildren())do
		if v:IsA("GuiObject") then
			Scale=v.AbsoluteSize.Y+0
		end
	end
	return Scale
end
CB.FocusLost:Connect(function(ep)
	if ep then
		LayedOut=LayedOut+1
		local Info = Label:Clone()
		Info.Text = "[".. User.DisplayName  .."] : " .. CB.Text
		Info.Parent = Main.InnerMain.Chat.ChatLogs
		Info.LayoutOrder = LayedOut
		Main.InnerMain.Chat.ChatLogs.CanvasSize = UDim2.new(0,0,0,GetLabels())
		Main.InnerMain.Chat.ChatLogs.CanvasPosition = Vector2.new(0,GetLabels())
		Info.Size = UDim2.new(0,Info.TextBounds.X,0,Info.TextBounds.Y)
		task.delay(60*5,function()
			Info:Destroy()
		end)
		SendChat(CB.Text)
		CB.Text = ""
	end
end)
Services.UIS.InputBegan:Connect(function(k,gp)
	if Services.UIS:GetFocusedTextBox() == nil and CB:IsFocused()==false then
		if k.KeyCode == Enum.KeyCode.Slash then
			task.defer(function()
				CB:CaptureFocus()
				CB.Text = ""
			end)
		end
	end
end)
Services.Players.PlayerAdded:Connect(DidChat)
for i,v in pairs(Services.Players:GetPlayers())do
	DidChat(v)
end
ExtraVariables.ActionTriggered.Event:Connect(function(b)
	ExtraVariables.ActionOpened = b
	if b == true then
		PlaySound(8379220604)
	end
end)

