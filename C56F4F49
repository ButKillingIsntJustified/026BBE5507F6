function AddToPlayerList(playerInstance)
	local FriendIcon = ""
	if playerInstance:IsFriendsWith(User.UserId) then
		FriendIcon = "ðŸ‘¤ "
	end
	local Label = ExtraVariables.PlayerLabel:Clone()
	Label.Parent = VrSurface.VrMain.InnerMain.Players
	Label.Text = FriendIcon .. playerInstance.Name
	Label.MouseEnter:Connect(function()
		Label.Text = FriendIcon .. playerInstance.Name
	end)
	Label.MouseLeave:Connect(function()
		Label.Text = FriendIcon .. playerInstance.DisplayName
	end)
	playerInstance:GetPropertyChangedSignal("TeamColor"):Connect(function()
		pcall(function()
			Label.BackgroundColor3 = playerInstance.TeamColor.Color
		end)
	end)
	Label.BackgroundColor3 = playerInstance.TeamColor.Color
	local selfex = nil
	selfex = Services.Players.PlayerRemoving:Connect(function(lplr)
		if lplr == playerInstance then
			Destroy(Label,-1)
			selfex:Disconnect()
		end
	end)
end
Players.PlayerAdded:Connect(AddToPlayerList)
for _,player in pairs(Players:GetPlayers())do
	AddToPlayerList(player)
end
