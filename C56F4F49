local VrTracker = {}

if game:GetService("RunService"):IsStudio() == false and Services.VR.VREnabled == false then return end

local FrameYield = function(Seconds)
	local Start = os.clock()
	repeat game:GetService("RunService").RenderStepped:Wait() until os.clock()-Start >= Seconds
	return os.clock()-Start
end

local ConnectFunctionUntil = function(Event,funcToLoop)
	local Temp = Event
	local Hmm = nil
	Hmm=game:GetService("RunService").Stepped:Connect(function()
		if funcToLoop() == true then
			Temp:Disconnect()
			Hmm:Disconnect()
		end
	end)
end
ExtraVariables.CalculatingFPS = false
ExtraVariables.LastFPS = "undefined"
function GetFPS()
	if ExtraVariables.CalculatingFPS == false then
		ExtraVariables.CalculatingFPS = true
		local Start = os.clock()
		local FPSC = 0
		repeat FPSC = FPSC + 1 game:GetService('RunService').RenderStepped:Wait() until os.clock()-Start >= 1
		ExtraVariables.CalculatingFPS = false
		ExtraVariables.LastFPS = FPSC
		return FPSC
	else
		return ExtraVariables.LastFPS
	end
end
local SpawnAlternative = function(func)
	task.spawn(func)
end

