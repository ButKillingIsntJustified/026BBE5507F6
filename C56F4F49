local User = game:GetService('Players').LocalPlayer
local AlternativeSpawn = task.spawn

local Part0 = Instance.new("Part")
local Highlight0 = Instance.new("Highlight",Part0)
local BillboardGui1 = Instance.new("BillboardGui")
local TextLabel2 = Instance.new("TextLabel")
Part0.Name = "ClickToInitiate"
Part0.CFrame = CFrame.new(7.5, 2, 24.125, 1, 0, 0, 0, 1, 0, 0, 0, 1)
Part0.Position = Vector3.new(7.5, 2, 24.125)
Part0.Color = Color3.new(0.972549, 0.972549, 0.972549)
Part0.Size = Vector3.new(0.5,0.5,0.5)
Part0.BottomSurface = Enum.SurfaceType.Smooth
Part0.BrickColor = BrickColor.new("Institutional white")
Part0.Material = Enum.Material.SmoothPlastic
Part0.TopSurface = Enum.SurfaceType.Smooth
Part0.brickColor = BrickColor.new("Institutional white")
BillboardGui1.Name = "InitiateBillboard"
BillboardGui1.Parent = Part0
BillboardGui1.LightInfluence = 1
BillboardGui1.Size = UDim2.new(8, 0, 0.5, 0)
BillboardGui1.Active = true
BillboardGui1.ClipsDescendants = true
BillboardGui1.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
BillboardGui1.Adornee = Part0
BillboardGui1.AlwaysOnTop = true
BillboardGui1.StudsOffset = Vector3.new(0, 1.2000000476837158, 0)
TextLabel2.Name = "InitiateLabel"
TextLabel2.Parent = BillboardGui1
TextLabel2.Size = UDim2.new(1, 0, 1, 0)
TextLabel2.BackgroundColor = BrickColor.new("Institutional white")
TextLabel2.BackgroundColor3 = Color3.new(1, 1, 1)
TextLabel2.BackgroundTransparency = 1
TextLabel2.Font = Enum.Font.TitilliumWeb
TextLabel2.FontSize = Enum.FontSize.Size14
TextLabel2.Text = "Click to initiate script"
TextLabel2.TextColor = BrickColor.new("Institutional white")
TextLabel2.TextColor3 = Color3.new(1, 1, 1)
TextLabel2.TextScaled = true
TextLabel2.TextSize = 14
TextLabel2.TextStrokeColor3 = Color3.new(0, 0.6, 1)
TextLabel2.TextStrokeTransparency = 0.5
TextLabel2.TextWrap = true
TextLabel2.TextWrapped = true
Highlight0.FillTransparency = 1
Highlight0.OutlineColor = Color3.fromRGB(0,150,255)
Highlight0.Adornee = Part0
local  ClickedInit = false
local CanClick = false
local SelfClick = nil
SelfClick = game:GetService('RunService').RenderStepped:Connect(function()
	Part0.Position = (workspace.CurrentCamera.CFrame*VR:GetUserCFrame(Enum.UserCFrame.Head)*CFrame.new(-0.5,-0.5,-2.5)).Position
	Part0.Anchored = true
	Part0.Velocity = Vector3.new(0,0,0)
	local yeah = workspace:GetPartBoundsInBox(workspace.CurrentCamera.CFrame*VR:GetUserCFrame(Enum.UserCFrame.LeftHand),Vector3.new(1,1,1),OverlapParams.new({['MaxParts']=100}))
	local yeah2 = workspace:GetPartBoundsInBox(workspace.CurrentCamera.CFrame*VR:GetUserCFrame(Enum.UserCFrame.RightHand),Vector3.new(1,1,1),OverlapParams.new({['MaxParts']=100}))
	if table.find(yeah,Part0) or table.find(yeah2,Part0) then
		CanClick = true
		BillboardGui1.Enabled = true
		Highlight0.Enabled = false
	else
		BillboardGui1.Enabled = false
		Highlight0.Enabled = true
		CanClick = false
	end
end)
Part0.Parent = workspace.CurrentCamera
local TempInput = nil
TempInput = game:GetService('UserInputService').InputBegan:Connect(function(key)
	if CanClick and (key.KeyCode == Enum.KeyCode.ButtonL2 or key.KeyCode == Enum.KeyCode.ButtonR2) then
		ClickedInit = true
		Highlight0.Enabled = false
		Highlight0:Destroy()
		Part0:Destroy()
		TempInput:Disconnect()
		SelfClick:Disconnect()
	end
end)
if game:GetService("RunService"):IsStudio() then
	ClickedInit = true
	Highlight0.Enabled = false
	Highlight0:Destroy()
	Part0:Destroy()
	TempInput:Disconnect()
	SelfClick:Disconnect()
end

repeat task.wait(-1) until ClickedInit==true

function Destroy(Obj,Delay_)
	if Delay_ then
		task.delay(Delay_,function()
			pcall(function()
				Obj:Destroy()
			end)
		end)
	else
		pcall(function()
			Obj:Destroy()
		end)
	end
end
local CreateTemporaryLoopWithRunServiceType = function(Type,Seconds,func)
	local Type = game:GetService("RunService")[Type]
	local Start = os.clock()
	if not Seconds then return warn("Please put a length in argument 2.") end
	local X = nil
	X = Type:Connect(function()
		if os.clock()-Start >= Seconds then
			X:Disconnect()
			return
		else
			func()
		end
	end)
end
local Create = function(Class,Parent,Properties)
	local A = Instance.new(Class)
	for i,v in pairs(Properties)do
		A[i]=v
	end
	A.Parent = Parent
	return A
end
local LastMessageInstances = {}
function PromptMessage(Message,lasts,BackgroundTransparency)
	for _,x in pairs(LastMessageInstances)do
		Destroy(x,-1)
	end
	if game:GetService("RunService"):IsStudio() then
		local H = Instance.new("Hint")
		H.Text = Message
		H.Parent = workspace
		task.delay(lasts,function()
			H.Parent=nil
		end)
		table.insert(LastMessageInstances,H)
		return
	end
	local Init_Message = Create("TextLabel",nil,{
		Text = Message,
		TextSize = 30,
		Font = Enum.Font.SourceSansBold,
		TextColor3 = Color3.new(1,1,1),
		BackgroundTransparency=1,
		Size = UDim2.new(1,0,1,0),
	})
	local Splash = Create("Part",workspace.CurrentCamera,{
		Size = Vector3.new(0,0,0),
		Anchored = true,
		CanCollide = false,
	})
	local BillboardGui = Create("BillboardGui",Splash,{
		Size = UDim2.new(20,0,20,0),
		AlwaysOnTop = true
	})
	local Darken = Create("ColorCorrectionEffect",game:GetService("Lighting"),{
		Brightness = -(1-BackgroundTransparency),
	})
	LastMessageInstances = {
		Init_Message,Splash,BillboardGui,Darken
	}
	Init_Message.Parent = BillboardGui
	CreateTemporaryLoopWithRunServiceType("RenderStepped",lasts,function()
		Splash.CFrame = workspace.CurrentCamera.CFrame*game:GetService('VRService'):GetUserCFrame(Enum.UserCFrame.Head) * CFrame.new(0,0,-10)
	end)
	task.wait(lasts)
	game:GetService('TweenService'):Create(Init_Message,TweenInfo.new(0.5),{
		Transparency=1,
		TextTransparency=1,
		TextStrokeTransparency=1,
	}):Play()
	game:GetService('TweenService'):Create(Darken,TweenInfo.new(0.5),{
		Brightness = 0,
	}):Play()
	Destroy(Init_Message,0.5)
	Destroy(BillboardGui,0.5)
	Destroy(Splash,0.5)
	Destroy(Darken,0.5)
end
for i,v in pairs(User:GetDescendants())do
	if v:IsA("LocalScript") then
		for _,n in pairs(Advanced.Scripts_To_Remove)do
			if v.Name:lower():find(n:lower()) then
				v.Disabled = true
			end
		end
	end
end
repeat task.wait(-1) until User.Character
for i,v in pairs(User.Character:GetDescendants())do
	if v:IsA("LocalScript") then
		for _,n in pairs(Advanced.Scripts_To_Remove)do
			if v.Name:lower():find(n:lower()) then
				v.Disabled = true
			end
		end
	end
end
AlternativeSpawn(function()
PromptMessage([[
...Checking your whitelist...

(Please do not spam execute)
]],30,0.5)
end)
	
